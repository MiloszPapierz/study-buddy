{
   "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
   "plugins": ["prettier"],
   "rules": {
      "prettier/prettier": [
         "error",
         {
            "endOfLine": "auto"
         }
      ]
   },
   "env": {
      "browser": true,
      "node": true,
      "es6": true
   },
   "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
         "experimentalObjectRestSpread": true
      }
   }
}
